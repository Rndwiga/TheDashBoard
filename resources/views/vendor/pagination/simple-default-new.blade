@if ($paginator->hasPages())
    <ul class="pagination>
        {{-- Previous Page Link --}}
        @if ($paginator->onFirstPage())
        <li><i class='fa fa-arrow-left icon-xs icon-orange icon-secondary disabled'></i></li>
        @else
        <li><a href="{{ $paginator->previousPageUrl() }}" rel="prev"><i class='fa fa-arrow-left icon-xs icon-orange icon-secondary'></i></a></li>
        @endif

        {{-- Next Page Link --}}
        @if ($paginator->hasMorePages())
            <li><a href="{{ $paginator->nextPageUrl() }}" rel="next"><i class='fa fa-arrow-right icon-xs icon-orange icon-secondary'></i></a></li>
        @else
            <li><i class='disabled fa fa-arrow-right icon-xs icon-orange icon-secondary'></i></li>
        @endif
    </ul>
@endif
